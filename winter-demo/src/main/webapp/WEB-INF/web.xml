<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
    <listener-class>
      com.winter.listen.WebContextLoaderListener
    </listener-class>
  </listener>
  <servlet>
    <servlet-name>loaderServlet</servlet-name>
    <servlet-class>com.bycuimiao.servlet.LoaderServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>loaderServlet</servlet-name>
    <url-pattern>/init</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>servletDemo</servlet-name>
    <servlet-class>com.bycuimiao.servlet.ServletDemo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servletDemo</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
</web-app>
